<h1 style="display:none;">JOBTOTOP</h1>

<header id="header" role="banner" class="nb">
	<div id="logo" style="margin-left: 50px; margin-top: 10px;">
	  <a href="{LINK_INDEX}" rel="home" title="{SITE_TITLE}">
		<img src="{SITE_URL}storage/logo/{SITE_LOGO}" alt="{SITE_TITLE}">
	  </a>
	</div>
  
    <nav id="nav" role="navigation">
  <div id="nav-inner">
    <ul>
      
		<li class="main active" id="nav-jobseeker">
		  <a href="{LINK_LISTING}" title=" {LANG_BROWSE_JOBS}">
			<svg class="icon"><use xlink:href="#icon-magglass"></use></svg>
			<span class="hide-m">
			   {LANG_BROWSE_JOBS}
			</span>
		  </a>
		</li>

		<li class="main offside" id="nav-post-cv">
		  <a href="{LINK_SIGNUP}" class="highlight" title="Post your CV">
			<svg class="icon"><use xlink:href="#icon-plus-round"></use></svg>
			<svg class="icon hide-s hide-xs"><use xlink:href="#icon-plus-round-filled"></use></svg>
			Post your CV
		  </a>
		</li>

		<li class="main adv" id="nav-advertiser">
		  <a href="{LINK_POST-JOB}" title="{LANG_POST_JOB}">
			<svg class="icon"><use xlink:href="#icon-megaphone"></use></svg>
			<span class="hide-m">
			  {LANG_POST_JOB}
			</span>
		  </a>
		</li>
		IF(!{LOGGED_IN}){
		<li id="nav-login">
		  <a href="#sign-in-dialog" class="btn btn-m btn-primary btn-r popup-with-zoom-anim" title="{LANG_LOGIN}">
			{LANG_LOGIN}
		  </a>
		</li>
		{:IF}
		

		IF({LOGGED_IN}){
		<li class="main" id="nav-user">
		  
		  <svg class="hide-s hide-xs icon icon-user"><use xlink:href="#icon-user"></use></svg>
		  <svg class="hide-s hide-xs icon icon-user icon-user-over"><use xlink:href="#icon-user2"></use></svg>
		  
		  <a class="hide-m hide-l" href="{LINK_DASHBOARD}">
			
			<svg class="icon icon-user"><use xlink:href="#icon-user"></use></svg>
			
			{USERNAME}
			
		  </a>
		  
			<ul class="user-menu-small-nav hide-s hide-xs">
				<li><a href="{LINK_DASHBOARD}"><i class="icon-feather-grid"></i> {LANG_DASHBOARD}
					</a></li>
				IF('{USERTYPE}' == "user"){
				IF({RESUME_ENABLE}){
				<li><a href="{LINK_RESUMES}"><i
								class="icon-feather-paperclip"></i> {LANG_MY_RESUMES}</a></li>
				{:IF}
				<li><a href="{LINK_APPLIED_JOBS}"><i class="icon-feather-briefcase"></i> {LANG_APPLIED_JOBS}</a>
				</li>
				<li><a href="{LINK_FAVJOBS}"><i class="icon-feather-heart"></i> {LANG_FAV_JOBS}</a>
				</li>
				<li><a href="{LINK_JOBALERT}"><i class="icon-feather-bell"></i> {LANG_JOB_ALERT}</a>
				</li>
				ELSEIF('{USERTYPE}' == "employer"){
				IF({COMPANY_ENABLE}){
				<li><a href="{LINK_MYCOMPANIES}"><i
								class="icon-feather-box"></i> {LANG_MY_COMPANIES}</a></li>
				{:IF}
				<li><a href="{LINK_MYJOBS}"><i class="icon-feather-briefcase"></i> {LANG_MY_JOBS}
					</a></li>
				<li><a href="{LINK_PENDINGJOBS}"><i
								class="icon-feather-clock"></i> {LANG_PENDING_JOBS}</a></li>
				<li><a href="{LINK_MEMBERSHIP}"><i class="icon-feather-gift"></i> {LANG_MEMBERSHIP}
					</a></li>
				<li><a href="{LINK_TRANSACTION}"><i
								class="icon-feather-file-text"></i> {LANG_TRANSACTIONS}</a></li>
				{:IF}
				IF('{WCHAT}'=='on'){
				<li><a href="{LINK_MESSAGE}"><i
								class="icon-feather-message-circle"></i> {LANG_MESSAGE}</a></li>
				{:IF}
				<li><a href="{LINK_LOGOUT}"><i class="icon-feather-log-out"></i> {LANG_LOGOUT}</a>
				</li>
			</ul>
			
		</li>
		{:IF}  
      
      
      
      
      
      
    </ul>
  </div>
</nav>

<div style="margin-top: 30px; " class="">
    <div class="btn-group bootstrap-select language-switcher">
      <a  class="badge badge-default badge-sm pull-right" data-toggle="dropdown" title="English"  style="background-color: #428ee6;">
    	<span class="filter-option pull-right" id="selected_lang">EN</span>&nbsp;
      </a>
      <div class="dropdown-menu scrollable-menu open">
          <ul class="dropdown-menu inner">
              {LOOP: LANGS}
                  <li data-lang="{LANGS.name}">
                      <a role="menuitem" tabindex="-1" rel="alternate"
                          href="{LINK_HOME}/{LANGS.code}">{LANGS.name}</a>
                  </li>
              {/LOOP: LANGS}
          </ul>
      </div>
    </div>
</div>
    <button id="nav-toggle" class="hide-m hide-l shift" data-target="#nav" type="button" title="Toggle menu">
  <svg class="icon icon-hamburger"><use xlink:href="#icon-hamburger"></use></svg>
  <svg class="icon icon-cross"><use xlink:href="#icon-cross"></use></svg>
</button>
  
</header>
<!-- Country Picker -->
    <div class="zoom-anim-dialog mfp-hide dialog-with-tabs popup-dialog big-dialog" id="countryModal">
        <ul class="popup-tabs-nav">
            <li><a href="#country"><i class="icon-feather-map-pin"></i> {LANG_SELECT_YOUR_COUNTRY}</a></li>
        </ul>
        <div class="popup-tabs-container">
            <div class="popup-tab-content" id="country">

                <div class="row">
                    <div class="col-md-6">
                        <div class="input-with-icon margin-bottom-30">
                            <input class="with-border" type="text" placeholder="{LANG_SEARCH}..." id="country-modal-search">
                            <i class="icon-feather-search"></i>
                        </div>
                    </div>
                    <ul id="countries" class="column col-md-12 col-sm-12 cities">
                        {LOOP: COUNTRYLIST}
                            <!--<li data-name="{COUNTRYLIST.name}"><span class="flag flag-{COUNTRYLIST.lowercase_code}"></span> <a
                                        href="{LINK_HOME}/{COUNTRYLIST.lang}/{COUNTRYLIST.lowercase_code}"
                                        data-id="{COUNTRYLIST.id}"
                                        data-name="{COUNTRYLIST.name}"> {COUNTRYLIST.name}</a></li> -->
                            <li data-name="{COUNTRYLIST.name}" style="padding:10px 0px">
							<img src="{SITE_URL}templates/{TPL_NAME}/images/flags/{COUNTRYLIST.lowercase_code}.png"/>&nbsp;&nbsp;&nbsp; <a
                                        href="{LINK_HOME}/{COUNTRYLIST.lang}/{COUNTRYLIST.lowercase_code}"
                                        data-id="{COUNTRYLIST.id}"
                                        data-name="{COUNTRYLIST.name}"> {COUNTRYLIST.name}</a></li>
                        {/LOOP: COUNTRYLIST}
                    </ul>
                </div>
            </div>
        </div>
    </div>
IF("{USERSTATUS}"=="0" && "{NON_ACTIVE_MSG}"=="1"){
    <div class="user-status-message">
        <div class="container">
            <div class="row">
                <div class="col-lg-8">
                    <i class="icon-lock text-18"></i>
                    <span>{LANG_WELCOME} <strong>{USERNAME}</strong>, {LANG_GOTO_UR_EMAIL} <strong>{USEREMAIL}</strong>  {LANG_VERIFY_EMAIL_ADDRESS}</span>
                </div>
                <div class="col-lg-4">
                    <a class="button ripple-effect" rel="nofollow" target="_blank" role="button" href="http://{EMAILDOMAIN}/">{LANG_GOTO_UR_EMAIL}</a>
                    <a class="button ripple-effect gray resend_buttons{USER_ID} resend" href='javascript:void(0);' id="{USER_ID}">{LANG_RESEND_EMAIL}</a>
                    <span class='resend_count' id='resend_count{USER_ID}'></span>
                </div>
            </div>
        </div>
    </div>
    {:IF}
<main id="main" role="main">
